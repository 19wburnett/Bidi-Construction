{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/cron/send-reminders",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/find-subcontractors",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/cron/process-vectorization-queue",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/enrich-subcontractors",
      "schedule": "*/30 * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/resend/webhook",
      "destination": "/api/resend/webhook",
      "has": [
        {
          "type": "header",
          "key": "content-type",
          "value": "application/json"
        }
      ]
    }
  ]
}












